"*":
  "atom-beautify":
    general:
      _analyticsUserId: "djreed"
      beautifyEntireFileOnSave: false
    ruby: {}
  "atom-fuzzy-grep":
    grepCommandString: "ack -i --nocolor --nogroup --column"
  "atom-material-ui":
    colors:
      abaseColor: "#7e57c2"
      accentColor: "#ffffff"
      predefinedColor: "Purple"
    fonts: {}
    tabs:
      compactTabs: true
    ui:
      panelContrast: true
      panelShadows: true
  "atomic-emacs": {}
  "autocomplete-plus":
    backspaceTriggersAutocomplete: true
  "autocomplete-ruby":
    rsensePath: "/Users/djreed/.rvm/gems/ruby-2.4.2/bin/rsense"
    suggestionPriority: true
  "city-lights-ui":
    showTabsInTreeView: true
  core:
    closeEmptyWindows: false
    destroyEmptyPanes: false
    disabledPackages: [
      "linter-eslint"
      "linter"
      "linter-ui-default"
      "flow-ide"
      "busy-signal"
      "language-rust"
      "hyperclick"
      "autocomplete-ruby"
    ]
    openEmptyEditorOnStart: false
    packagesWithKeymapsDisabled: []
    telemetryConsent: "no"
    themes: [
      "atom-material-ui"
      "monokai"
    ]
  "dbg-gdb":
    logToConsole: true
  editor:
    autoIndent: false
    fontSize: 12
    invisibles: {}
    showIndentGuide: true
    tabType: "soft"
    zoomFontWhenCtrlScrolling: true
  "emacs-plus": {}
  "exception-reporting":
    userId: "86c92ee3-52e7-4de5-ba8b-fad388245b3f"
  "fuzzy-finder": {}
  "git-blame":
    colorCommitAuthors: true
    columnWidth: 247
    ignoreWhiteSpaceDiffs: true
  "go-plus":
    autocomplete:
      suppressBuiltinAutocompleteProvider: false
    config: {}
    format: {}
    panel:
      fontSize: "1em"
    test: {}
  "highlight-selected": {}
  "linter-eslint":
    disableFSCache: true
  "linter-haml": {}
  "linter-rubocop":
    disableWhenNoConfigFile: true
  "linter-ui-default":
    panelHeight: 205
  minimap:
    absoluteMode: true
    adjustAbsoluteModeHeight: true
    moveCursorOnMinimapClick: true
    plugins:
      codeglance: false
      codeglanceDecorationsZIndex: 0
      cursorline: true
      cursorlineDecorationsZIndex: 0
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      "minimap-autohider": true
      "minimap-autohiderDecorationsZIndex": 0
      "minimap-hide": true
      "minimap-hideDecorationsZIndex": 0
      "minimap-quick-highlight": true
      "minimap-quick-highlightDecorationsZIndex": 0
      pigments: true
      pigmentsDecorationsZIndex: 0
      selection: true
      selectionDecorationsZIndex: 0
    scrollAnimation: true
  "platformio-ide-terminal":
    core:
      scrollback: 100000
    toggles:
      autoClose: true
      runInsertedText: false
  "quick-highlight":
    countDisplayPosition: "Right"
    displayCountOnStatusBar: false
  "ruby-block":
    highlightLineNumber: true
  "ruby-solargraph": {}
  "seti-ui":
    compactView: true
  welcome:
    showOnStartup: false
  whitespace:
    ensureSingleTrailingNewline: false
    ignoreWhitespaceOnlyLines: true
    removeTrailingWhitespace: false
".erb.html.text":
