"*":
  "activate-power-mode":
    comboMode: {}
    particles:
      colours:
        type: "random"
    playAudio:
      volume: 0.25
    plugins:
      playAudio: true
      screenShake: true
    screenShake:
      enabled: false
  "atom-beautify":
    general:
      _analyticsUserId: "djreed"
      beautifyEntireFileOnSave: false
    ruby: {}
  "atom-fuzzy-grep":
    grepCommandString: "ack -i --nocolor --nogroup --column"
  "atom-ide-ui":
    "atom-ide-code-format":
      formatOnSave: true
  "atom-material-ui":
    colors:
      abaseColor: "#7c57c2"
      accentColor: "#fefdff"
      predefinedColor: "Purple"
    fonts: {}
    tabs:
      compactTabs: true
    ui:
      panelContrast: true
      panelShadows: true
  "atomic-emacs": {}
  "autocomplete-plus":
    backspaceTriggersAutocomplete: true
    enableExtendedUnicodeSupport: true
  "autocomplete-ruby":
    rsensePath: "/Users/djreed/.rvm/gems/ruby-2.4.2/bin/rsense"
    suggestionPriority: true
  "city-lights-ui":
    showTabsInTreeView: true
  core:
    closeEmptyWindows: false
    destroyEmptyPanes: false
    disabledPackages: [
      "activate-power-mode"
    ]
    openEmptyEditorOnStart: false
    packagesWithKeymapsDisabled: []
    telemetryConsent: "no"
    themes: [
      "atom-material-ui"
      "monokai"
    ]
  "dbg-gdb":
    logToConsole: true
  editor:
    autoIndent: false
    fontSize: 12
    invisibles: {}
    showIndentGuide: true
    showInvisibles: true
    tabType: "soft"
    zoomFontWhenCtrlScrolling: true
  "emacs-plus": {}
  "exception-reporting":
    userId: "86c92ee3-52e7-4de5-ba8b-fad388245b3f"
  "fuzzy-finder":
    ignoredNames: [
      "**/node_modules/*"
    ]
  "git-blame":
    colorCommitAuthors: true
    columnWidth: 247
    ignoreWhiteSpaceDiffs: true
  "go-plus":
    autocomplete:
      suppressBuiltinAutocompleteProvider: false
    config:
      compileOnSave: false
    format: {}
    panel:
      fontSize: "1em"
    test:
      coverageDisplayMode: "gutter"
  "highlight-selected": {}
  "linter-eslint":
    advanced:
      disableFSCache: true
  "linter-haml": {}
  "linter-rubocop":
    disableWhenNoConfigFile: true
  "linter-ui-default":
    panelHeight: 300
  minimap:
    displayCodeHighlights: false
    moveCursorOnMinimapClick: true
    plugins:
      codeglance: false
      codeglanceDecorationsZIndex: 0
      cursorline: true
      cursorlineDecorationsZIndex: 0
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
